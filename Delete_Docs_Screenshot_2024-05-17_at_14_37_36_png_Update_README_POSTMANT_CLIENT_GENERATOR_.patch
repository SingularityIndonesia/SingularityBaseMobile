+fun File.find(
+    clue: String
+): Sequence<File> {
+    return listFiles()
+        ?.asSequence()
+        ?.mapNotNull { file ->
+            when {
+                !file.isDirectory && file.name.contains(clue) -> sequenceOf(file)
+                file.isDirectory -> file.find(clue)
+                else -> null
+            }
+        }
+        ?.flatten()
+        ?: sequenceOf()
+}
